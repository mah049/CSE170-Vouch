    <div class="container-fluid">
    	<div class="row" id="buffer"></div>
		<div class="row top-buffer"> 
		<div class="col-sm-6 col-sm-offset-3">
            <div id="imaginary_container"> 
                <div class="input-group stylish-input-group">
                    <input type="search" class="form-control" id="search" placeholder="search" >
                    <span class="input-group-addon">
                        <button type="submit">
                            <span class="glyphicon glyphicon-search"></span>
                        </button>
                    </span>
                </div>
                <div id="update"></div>
            </div>
        </div>
		  	<div class="col-md-4"> 
		  	</div>
        </div>
	  	<div class="row" id="small-buffer"></div>
	  	<div class="row">
		  	<div class="col-md-2" id="left-side-margin"> 
		  		<p>Filter</p>
				<form>
					<div class="radio">
				      <label><input type="radio" name="optradio" id="mostPopular" {{mostPopular}}>Most Popular</label>
				    </div>
				    <div class="radio">
				      <label><input type="radio" name="optradio" id="mostRecent" {{mostRecent}}>Most Recent</label>
				    </div>
				    <div class="radio">
				      <label><input type="radio" name="optradio" id="leastDistance" disabled>Least Distance</label>
				    </div>
				</form>
				<p>Categories</p>
        <form action="/categories" method="get" id="categories">
  				<div class="checkbox">
    					<label><input type="checkbox" class="check" name="all" id="checkAll" {{all}}>All</label>
  				</div>
  				<div class="checkbox">
    					<label><input type="checkbox" class="check" name="restaurants" {{restaurants}}>Restaurants</label>
  				</div>
  				<div class="checkbox">
    					<label><input type="checkbox" class="check" name="events" {{events}}>Events</label>
  				</div>
  				<div class="checkbox">
    					<label><input type="checkbox" class="check" name="groceries" {{groceries}}>Groceries</label>
  				</div>
        </form>
	  		</div>
		  	<div class="col-sm-8" align = "middle"> 
		      	<!-- This is where the image used to be  
				<a href="dealView.html">
		      		<img src="images/exampleDeal.png" alt="HTML5 icon" class="img-responsive">
		      	</a>
				-->
		    <div id="deal-container">
	  		{{#each deal}}
				<a id="deals" href="/dealView/{{Place}}">
					<div class="well">
				<p id="newDeals">{{Place}}<br>{{Deal}}<br>{{Upvote}} Vouches</p>
					</div>
				</a>
		    {{/each}}
		    </div>
		    </div>
		    <div class="col-sm-2" align="right" id="margin-right-align"> 
		    </div>
	  	</div>
    </div>
